{
  "name": "starter-kit",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "heroku:postbuild": "yarn ba",
    "start": "node ./dist/server.js",
    "build:client": "webpack --config ./webpack/client.config.js",
    "build:server": "webpack --config ./webpack/server.config.js",
    "build:all": "concurrently -p index 'yarn bc' 'yarn bs'",
    "bc": "yarn build:client",
    "bs": "yarn build:server",
    "ba": "yarn build:all",
    "dev": "concurrently 'yarn dev:server' 'yarn dev:client'",
    "dev:server": "nodemon -e js,ts,tsx --use_strict --inspect=9229 entry/server --exec ts-node",
    "dev:client": "node -e 'require(\"./webpack/webpack-dev-server\").startDevServer()'",
    "lint": "tslint -c ./tslint.json './modules/**/*.ts*'",
    "migrate:up": "PG_DEV_URL=\"postgres://dev:dev@localhost:5433/msl-dev\" knex migrate:latest",
    "migrate:down": "PG_DEV_URL=\"postgres://dev:dev@localhost:5433/msl-dev\" knex migrate:rollback",
    "test:c": "NODE_ENV=test mocha --compilers ts:ts-node/register,tsx:ts-node/register --recursive client/tests",
    "test:s": "NODE_ENV=test mocha --compilers ts:ts-node/register --recursive server/tests",
    "pretest": "yarn lint",
    "test": "jest",
    "test:debug": "node --debug-brk ./node_modules/.bin/jest --runInBand",
    "ta": "yarn test:all",
    "before:commit": "yarn ba && yarn lint && yarn test:all",
    "bcom": "yarn before:commit",
    "db:create:dev": "createdb msl-dev",
    "db:create:test": "createdb msl-test"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/config": "^0.0.32",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^2.0.20",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.61",
    "@types/node": "^7.0.8",
    "@types/react": "^15.0.21",
    "@types/react-addons-test-utils": "^0.14.17",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.38",
    "@types/react-router": "^4.0.4",
    "@types/react-router-dom": "^4.0.4",
    "@types/react-router-redux": "^5.0.0",
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "^3.1.2",
    "body-parser": "^1.17.1",
    "config": "^1.25.1",
    "cors": "^2.8.3",
    "cross-env": "^3.2.4",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "graphql": "^0.9.3",
    "graphql-server-express": "^0.7.1",
    "graphql-tools": "^0.11.0",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.2",
    "pg": "^6.1.5",
    "postcss-loader": "^1.3.3",
    "postcss-modules-local-by-default": "^1.1.1",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-saga": "https://github.com/dcolthorp/redux-saga.git#update-typings",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.16.1",
    "ts-node": "^3.0.0",
    "tslint": "^5.1.0",
    "tslint-eslint-rules": "^4.0.0",
    "typescript": "^2.3.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4",
    "webpack-split-by-path": "^2.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^2.7.6",
    "@types/jest": "^19.2.2",
    "concurrently": "^3.4.0",
    "enzyme": "^2.8.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "jest-environment-node-debug": "^2.0.0",
    "nodemon": "^1.11.0",
    "ts-jest": "^19.0.10",
    "webpack-dev-server": "^2.4.2"
  }
}
